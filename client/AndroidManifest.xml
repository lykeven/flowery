<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.identification"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"             
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.project.identification.GuideActivity"
            android:label="@string/app_name" >            
        </activity>
        
        <activity android:name="Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".NormalActivity"  />
        <activity android:name="MainActivity"></activity>
         <activity
            android:name="com.project.identification.Display"
            android:label="@string/app_show"   >
        </activity>
        <activity
            android:name="com.project.identification.TakeCamera"
            android:label="@string/app_name"   >
        </activity>
        <activity
            android:name="com.project.identification.GetInfo"
            android:label="@string/app_name"   >
        </activity>
     
    </application>

</manifest>